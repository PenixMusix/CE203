package shapes;

public class Rectangle extends Shape {
    protected int x;
    protected int y;
    protected int width;
    protected int height;

    public Rectangle(int x, int y, int width, int height) {
        this.name = "rectangle";
        this.x = x;
        this.y = y;
        this.width = width;
        this.height = height;
    }

    public void draw() {
        System.out.println("This is a rectangle with location(" + x + "," + y + ") and size " + width + "x" + height);
    }
}
